import java.util.Scanner;

class EmployeePayroll {

    double calculateSalary(int employeeID) {
        return 0;
    }

    double calculateSalary(int hoursWorked, double rate) {
        return hoursWorked * rate;
    }

    double calculateSalary(int employeeID, double bonus) {
        return bonus;
    }

    double calculateSalary(double annualSalary) {
        return annualSalary / 12;
    }

    double calculateSalary(int hoursWorked, double rate, int overtimeHours) {
        return (hoursWorked * rate) + (overtimeHours * rate * 1.5);
    }

    double calculateSalary(double salary, double deductions) {
        return salary - deductions;
    }

    double calculateSalary(double salary, double bonus, double deductions) {
        return salary + bonus - deductions;
    }
}

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        EmployeePayroll ep = new EmployeePayroll();

        System.out.println(ep.calculateSalary(101));
        System.out.println(ep.calculateSalary(40, 500));
        System.out.println(ep.calculateSalary(102, 2000));
        System.out.println(ep.calculateSalary(600000));
        System.out.println(ep.calculateSalary(40, 500, 5));
        System.out.println(ep.calculateSalary(30000, 2000));
        System.out.println(ep.calculateSalary(30000, 5000, 2000));
    }
}
